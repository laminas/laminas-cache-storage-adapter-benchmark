{
    "checks": [
        {
            "name": "PHPBench",
            "job": {
                "php": "*",
                "dependencies": "latest",
                "command": "./vendor/bin/phpbench run --revs=2 --iterations=2 --report=aggregate"
            }
        }
    ],
    "ignore_php_platform_requirements": {
        "8.2": true
    }
}
